{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "Android/api-reference/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "rome_bread"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
        "feedback_system": "GitHub",
        "feedback_github_repo": "MicrosoftDocs/project-rome",
        "uhfHeaderId": "MSDocsHeader-WinUWP",
        "breadcrumb_path":  "~/breadcrumb/toc.yml",
        "extendBreadcrumb": true,
        "titleSuffix": "Universal Windows Platform",
        "ms.topic": "article",
        "ms.prod": "windows",
        "ms.technology": "Project Rome", 
        "ms.date": "09/27/2017", 
        "ms.author": "pafarley",
        "author": "PatrickFarley"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "project-rome"
  }
}
